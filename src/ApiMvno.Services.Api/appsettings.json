{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=$DB_HOST;Database=$DB_NAME;User ID=$DB_USER;Password=$DB_PASSWORD;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Jwt": {
    "SecretKey": "$JWT_SECRET_KEY",
    "Issuer": "$JWT_ISSUER",
    "Audience": "$JWT_AUDIENCE"
  },
  "RedisOptions": {
    "Connection": "$CONNECTION_REDIS,password=$CONNECTION_REDIS_PASS",
    "InstanceName": "mvnoapi",
    "ApplicationName": "mvnoapi"
  },
  "MailApi": {
    "Url": "$MAIL_URL",
    "Token": "$MAIL_TOKEN"
  },
  "SmsApi": {
    "Url": "$SMS_URL",
    "Email": "$SMS_USER_NAME",
    "Password": "$SMS_PASSWORD",
    "Enabled": false
  },
  "General": {
    "FrontUrl": "$FRONTEND_URL"
  }
}
